    "jquery": "global:$"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "babelify": "^6.1.3",
    "browserify": "^10.2.1",
    "browserify-shim": "^3.8.10",
    "gulp": "^3.8.11",
    "gulp-mocha": "^2.1.3",
    "gulp-util": "^3.0.4",
    "mocha": "^2.2.5",
    "react": "^0.13.3",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.2.1"
  }
}
